<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="449.0" prefWidth="641.0" spacing="10"  xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <HBox alignment="TOP_RIGHT" prefHeight="17.0" prefWidth="641.0">
      <children>
         <Button fx:id="AccueilButton" mnemonicParsing="false" text="Accueil" />
      </children>
   </HBox>
    <Label id="loginTitle" text="Login" />
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
          <TextField fx:id="telephoneField" promptText="Telephone Number" />
      </children>
   </HBox>
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
          <PasswordField fx:id="passwordField" promptText="Password" />
      </children>
   </HBox>
    <Button fx:id="loginButton" text="Login" />
    <Button fx:id="creat" text="Create Account" />
</VBox>
